{
  "globals"   : {
    "describe"   : false,
    "it"         : false,
    "before"     : false,
    "beforeEach" : false,
    "after"      : false,
    "afterEach"  : false
  },
  "camelcase"     : true,
  "curly"         : true,
"eqeqeq"          : true,
  "esversion"     : 6,
  "freeze"        : true,
  "globalstrict"  : true,
  "indent"        : 2,
  "latedef"       : true,
  "laxcomma"      : true,
  "node"          : true,
  "noyield"       : true,
  "trailing"      : true,
  "strict"        : true,
  "unused"        : "vars",
  "undef"         : true
}
